{
  "name": "xosedev-portfolio-orchestrator",
  "version": "1.0.0",
  "description": "xosedev-portfolio-orchestrator",
  "main": "initServer.js",
  "scripts": {
    "start": "node dist/initServer.js",
    "compile": "ts-node build.ts && tsc",
    "build": "ts-node build.ts && tsc",
    "build:prod": "ts-node build.ts && tsc",
    "dev": "nodemon src/initServer.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' src/initServer.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js '{src, tests}/**/*.{js,ts,tsx}' --quiet",
    "lint:fix": "eslint -c .eslintrc.js '{src, tests}/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest -i --detectOpenHandles --testPathPattern=tests/unit",
    "test:unit": "jest -i --silent --testPathPattern=tests/unit",
    "test:integration": "jest -i --silent --testPathPattern=tests/integration",
    "test:coverage": "jest -i --silent --coverage",
    "local:test:unit": "jest -i --watchAll --detectOpenHandles --testPathPattern=tests/unit",
    "local:test:coverage": "jest -i --silent --watchAll --coverage --detectOpenHandles"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^3.10.1",
    "@types/lodash": "^4.14.181",
    "axios": "^0.27.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-and-time": "^2.0.1",
    "dd-trace": "^1.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-actuator": "^1.5.0",
    "express-openapi-validator": "^3.12.9",
    "express-validator": "^6.14.1",
    "glob": "^7.1.6",
    "http-status": "^1.4.2",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.5",
    "pino": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "sanitize": "^2.1.0",
    "serialize-error": "^10.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/date-and-time": "^0.13.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pino": "^6.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "class-validator": "^0.13.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^27.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "msw": "^0.39.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "shelljs": "^0.8.4",
    "sinon": "^9.2.0",
    "sinon-stub-promise": "^4.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^8.10.1",
    "typescript": "^4.1.0"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml"
  },
  "author": ""
}
